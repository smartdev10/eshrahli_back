<html>
  <head>
    <title>Show Result Demo</title>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0"
    />

    <link
      rel="shortcut icon"
      href="https://goSellJSLib.b-cdn.net/v1.4.1/imgs/tap-favicon.ico"
    />
    <link
      href="https://goSellJSLib.b-cdn.net/v1.4.1/css/gosell.css"
      rel="stylesheet"
    />
  </head>
  <body>

    <script
      type="text/javascript"
      src="https://goSellJSLib.b-cdn.net/v1.4.1/js/gosell.js"
    ></script>

    <div id="root"></div>
    <script>
       goSell.showResult({
           callback: response => {
           console.log("callback", response);
           fetch('/api/requests/reference/'+response.callback.metadata.id , {method:'PUT' , body :{payementReference:response.callback.reference.payment,status:"Paid"}})
            .then(response => response.json())  // convert to json
            .then(json => console.log(json))    //print data to console
            .catch(err => console.log('Request Failed', err)); // Catch errors
         }
      });
    </script>
  </body>
</html>